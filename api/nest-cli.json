{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/company/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/company/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/company",
  "projects": {
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "appointment": {
      "type": "application",
      "root": "apps/appointment",
      "entryFile": "main",
      "sourceRoot": "apps/appointment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/appointment/tsconfig.app.json"
      }
    },
    "job-advertisement": {
      "type": "application",
      "root": "apps/job-advertisement",
      "entryFile": "main",
      "sourceRoot": "apps/job-advertisement/src",
      "compilerOptions": {
        "tsConfigPath": "apps/job-advertisement/tsconfig.app.json"
      }
    },
    "company": {
      "type": "application",
      "root": "apps/company",
      "entryFile": "main",
      "sourceRoot": "apps/company/src",
      "compilerOptions": {
        "tsConfigPath": "apps/company/tsconfig.app.json"
      }
    },
    "authentication": {
      "type": "application",
      "root": "apps/authentication",
      "entryFile": "main",
      "sourceRoot": "apps/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentication/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}
